<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MultiFunctionView">
        <attr name="showCheckBox" format="boolean" />
        <attr name="isCheck" format="boolean" />
        <attr name="checkBoxWidth" format="dimension" />
        <attr name="checkboxBackground" format="reference" />
        <attr name="checkboxLeftMargin" format="dimension" />
        <attr name="checkboxRightMargin" format="dimension" />
        <attr name="showLeftImage" format="boolean" />
        <attr name="leftImageBackground" format="reference" />
        <attr name="leftImageSrc" format="reference" />
        <attr name="leftImageWidth" format="dimension" />
        <attr name="leftImageScaleType" format="enum">
            <enum name="matrix" value="0" />
            <enum name="fitXY" value="1" />
            <enum name="fitStart" value="2" />
            <enum name="fitCenter" value="3" />
            <enum name="fitEnd" value="4" />
            <enum name="center" value="5" />
            <enum name="centerCrop" value="6" />
            <enum name="centerInside" value="7" />
        </attr>
        <attr name="leftImageLeftMargin" format="dimension" />
        <attr name="leftImageRightMargin" format="dimension" />
        <attr name="topTitle" format="string" />
        <attr name="topTitleHint" format="string" />
        <attr name="topTitleBackground" format="reference" />
        <attr name="topTitleSize" format="dimension" />
        <attr name="topTitleColor" format="color|reference" />
        <attr name="topTitleHintColor" format="color|reference" />
        <attr name="topTitleMaxLine" format="integer" />
        <attr name="topTitleWpfStyle" format="enum">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
        </attr>
        <attr name="bottomTitle" format="string" />
        <attr name="bottomTitleHint" format="string" />
        <attr name="bottomTitleBackground" format="reference" />
        <attr name="bottomTitleSize" format="dimension" />
        <attr name="bottomTitleColor" format="color|reference" />
        <attr name="bottomTitleHintColor" format="color|reference" />
        <attr name="bottomTitleMaxLine" format="integer" />
        <attr name="bottomTitleWpfStyle" format="enum">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
        </attr>
        <attr name="bottomTitleMarginTop" format="dimension" />
        <attr name="showRightImage" format="boolean" />
        <attr name="rightImageBackground" format="reference" />
        <attr name="rightImageSrc" format="reference" />
        <attr name="rightImageWidth" format="dimension" />
        <attr name="rightImageScaleType" format="enum">
            <enum name="matrix" value="0" />
            <enum name="fitXY" value="1" />
            <enum name="fitStart" value="2" />
            <enum name="fitCenter" value="3" />
            <enum name="fitEnd" value="4" />
            <enum name="center" value="5" />
            <enum name="centerCrop" value="6" />
            <enum name="centerInside" value="7" />
        </attr>
        <attr name="rightImageLeftMargin" format="dimension" />
        <attr name="rightImageRightMargin" format="dimension" />
        <attr name="clickView" format="enum">
            <enum name="itemView" value="0" />
            <enum name="checkBox" value="1" />
            <enum name="leftImage" value="2" />
            <enum name="title" value="3" />
            <enum name="subTitle" value="4" />
            <enum name="rightImage" value="5" />
        </attr>
        <attr name="clickOnlyCheckBox" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AspectRatioView">
        <attr name="ratio" format="float" />
        <attr name="isWidthMain" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ViewShadow">
        <attr name="key" format="string" />
        <attr name="bindTypes" format="flags">
            <flag name="visibility" value="0x1" />
            <flag name="background" value="0x2" />
            <flag name="text" value="0x4" />
            <flag name="textSize" value="0x8" />
            <flag name="textColor" value="0x10" />
            <flag name="textColorStateList" value="0x20" />
            <flag name="isSelect" value="0x40" />
            <flag name="checked" value="0x80" />
            <flag name="image" value="0x100" />
        </attr>
    </declare-styleable>

    <declare-styleable name="BottomTabLayout">
        <attr name="animView" format="reference" />
    </declare-styleable>

    <declare-styleable name="LoadSp2Text">
        <attr name="fileName" format="string" />
        <attr name="bindKey" format="string" />
        <attr name="defaultString" format="string" />
        <attr name="bindData2Sp" format="boolean" />
    </declare-styleable>

    <declare-styleable name="QuickLinearLayout">
        <attr name="itemSpace" format="dimension" />
        <attr name="spaceType" />
    </declare-styleable>

    <declare-styleable name="AutoTagLayout">
        <attr name="tagDirection">
            <enum name="row" value="0" />
            <enum name="row_reverse" value="1" />
            <enum name="column" value="2" />
            <enum name="column_reverse" value="3" />
        </attr>

        <attr name="tagWrap">
            <enum name="nowrap" value="0" />
            <enum name="wrap" value="1" />
            <enum name="wrap_reverse" value="2" />
        </attr>

        <!--
            Omitting flex-flow property since it's reflected in the parent flex container.
            Set the flexDirection and/or flexWrap to the parent flex container explicitly if you
            want to use the flex-flow similar way to the web.
         -->

        <attr name="tagJustifyContent">
            <enum name="flex_start" value="0" />
            <enum name="flex_end" value="1" />
            <enum name="center" value="2" />
            <enum name="space_between" value="3" />
            <enum name="space_around" value="4" />
            <enum name="space_evenly" value="5" />
        </attr>

        <attr name="tagAlignItems">
            <enum name="flex_start" value="0" />
            <enum name="flex_end" value="1" />
            <enum name="center" value="2" />
            <enum name="baseline" value="3" />
            <enum name="stretch" value="4" />
        </attr>

        <attr name="tagAlignContent">
            <enum name="flex_start" value="0" />
            <enum name="flex_end" value="1" />
            <enum name="center" value="2" />
            <enum name="space_between" value="3" />
            <enum name="space_around" value="4" />
            <enum name="stretch" value="5" />
        </attr>

        <!--
            The attribute that specifies the maximum number of flex lines. This attribute is
            effective only when the flexWrap attribute is "wrap" or "wrap_reverse".
        -->
        <attr name="maxLine" format="integer" />
    </declare-styleable>

    <declare-styleable name="QuickTitleView">
        <attr name="isLinearLayout" format="boolean" />
        <attr name="isAbsoluteCenter" format="boolean" />
        <attr name="showBackIcon" format="boolean" />
        <attr name="contentGravity" format="enum">
            <enum name="center" value="0"/>
            <enum name="start" value="1"/>
            <enum name="end" value="2"/>
        </attr>
        <attr name="backIcon" format="reference" />
        <attr name="titleStr" format="string" />
        <attr name="titleColor" format="color|reference" />
        <attr name="titleBold" format="boolean" />
        <attr name="titleSize" format="dimension" />
        <attr name="titleSpace" format="dimension" />
        <attr name="subTitleStr" format="string" />
        <attr name="subTitleColor" format="color|reference" />
        <attr name="subTitleBold" format="boolean" />
        <attr name="subTitleSize" format="dimension" />
        <attr name="showLine" format="boolean" />
        <attr name="space" format="dimension" />
    </declare-styleable>
</resources>