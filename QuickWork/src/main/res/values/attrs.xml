<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="QuickTitleView">
        <attr name="isLinearLayout" format="boolean" />
        <attr name="isAbsoluteCenter" format="boolean" />
        <attr name="showBackIcon" format="boolean" />
        <attr name="contentGravity" format="enum">
            <enum name="center" value="0"/>
            <enum name="start" value="1"/>
            <enum name="end" value="2"/>
        </attr>
        <attr name="backIcon" format="reference" />
        <attr name="titleStr" format="string" />
        <attr name="titleColor" format="color|reference" />
        <attr name="titleBold" format="boolean" />
        <attr name="titleSize" format="dimension" />
        <attr name="titleSpace" format="dimension" />
        <attr name="subTitleStr" format="string" />
        <attr name="subTitleColor" format="color|reference" />
        <attr name="subTitleBold" format="boolean" />
        <attr name="subTitleSize" format="dimension" />
        <attr name="showLine" format="boolean" />
        <attr name="space" format="dimension" />
    </declare-styleable>
</resources>