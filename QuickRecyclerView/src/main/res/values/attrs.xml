<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="QuickRecyclerView">
        <attr name="space" format="dimension" />
        <attr name="spaceType" />
        <attr name="includeFirst" format="boolean" />
        <attr name="includeLast" format="boolean" />
    </declare-styleable>
    <attr name="isMatch" format="boolean" />
    <declare-styleable name="QuickHeaderShadow">
        <attr name="isSuspension" format="boolean" />
        <attr name="isMatch" />
    </declare-styleable>
    <declare-styleable name="QuickFooterShadow">
        <attr name="isMatch" />
    </declare-styleable>

    <attr name="spaceType" format="enum">
        <enum name="center" value="0" />
        <enum name="start" value="1" />
        <enum name="end" value="2" />
    </attr>

    <declare-styleable name="SwipeMenuLayout">
        <attr name="swipeEnable" format="boolean" />
        <attr name="ios" format="boolean" />
        <attr name="leftSwipe" format="boolean" />
    </declare-styleable>
</resources>